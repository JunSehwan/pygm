{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "next",
    "next/babel",
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "quotes": ["error", "double"],
    "@next/next/no-img-element": "off"
  }
}
